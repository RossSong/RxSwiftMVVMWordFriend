source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '9.0'
use_frameworks!

def myPods
    pod 'RxSwift'
    pod 'RxCocoa'
    pod 'Alamofire'
    pod 'AlamofireImage'
    pod 'RxAlamofire'
    pod 'RealmSwift'
    pod 'Swinject'
    pod 'ReactorKit'
end

target 'RxSwiftWordFriend' do
	myPods
end

target 'RxSwiftWordFriendTests' do
	myPods
    pod 'Quick'
    pod 'Nimble'
end
